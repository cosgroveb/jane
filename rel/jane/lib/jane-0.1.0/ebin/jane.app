{application,jane,
             [{description,"jane: just another erlang bot"},
              {vsn,"0.1.0"},
              {modules,[jane_app,jane_server,jane_sup]},
              {registered,[jane_sup]},
              {applications,[kernel,stdlib]},
              {mod,{jane_app,[]}},
              {env,[{user_login,"jane@localhost"},
